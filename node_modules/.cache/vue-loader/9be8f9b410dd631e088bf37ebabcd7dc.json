e)\n          if (!readTime) {\n            readTime = 0\n          }\n          this.task = setInterval(() => {\n            readTime++\n            if (readTime % 30 === 0) {\n              saveReadTime(this.fileName, readTime)\n            }\n          }, 1000)\n        }\n      },\n      mounted() {\n        this.startLoopReadTime()\n      },\n      beforeDestroy() {\n        if (this.task) {\n          clearInterval(this.task)\n        }\n      }\n    }\n</script>\n\n<style lang=\"scss\" rel=\"stylesheet/scss\" scoped>\n  @import \"../../assets/styles/global\";\n</style>\n"]}]}